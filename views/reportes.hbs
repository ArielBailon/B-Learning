<h5>
  Informe diario ESANMA</h5>
<h5>{{fechaReporte}}</h5>
<h5>Condici√≥n operativa de las aeronaves</h5>
<br>


<div class="text-nowrap">

  <table id="data_table" class="table table-hover table-sm .table-bordered " border="2">
    <thead style="background:#626262; color:#fff">
      <tr>
        <th scope="col">#</th>
        <th scope="col">Aeronave</th>
        <th scope="col">Modelo</th>
        <th scope="col">Serie</th>
        <th scope="col">Fabricante</th>
        <th scope="col">Horas disponibles</th>
        <th scope="col">Horas de Vuelo</th>
        <th scope="col">Observaciones</th>
        <th scope="col">Fecha de Reporte</th>
      </tr>
    </thead>
    <tbody>
      {{#each aeronaves}}
      <tr>
        <th scope="row">{{counter @index}}</th>
        <td>{{nombre}}</td>
        <td>{{modelo}}</td>
        <td>{{serie}}</td>
        <td>{{fabricante}}</td>
        <td>{{horasActuales}}</td>
        <td>{{horasVoladas}}</td>
        <td style="">
          {{#observaciones}}
          <table>
            <tr>
              <td>
                <p>
                  {{descripcion}}
                </p>
              </td>
            </tr>
          </table>
          {{/observaciones}}
        </td>
        <td style="">
          {{#observaciones}}
          <table>
            <tr>
              <td>
                <p>
                  {{fecha}}
                </p>
              </td>
            </tr>
          </table>
          {{/observaciones}}
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>

<div class="mx-auto" style="width: 200px;">
  <button style="" type="button" class="btn btn-primary" onclick="window.print()">
    Imprimir
  </button>
</div>